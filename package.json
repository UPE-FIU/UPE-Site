{
  "name": "upe-website",
  "version": "1.0.0",
  "private": true,
  "description": "UPE Website",
  "author": "apthos",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "prettier": "prettier --ignore-path .gitignore -w .",
    "precommit": "npm run type-check && npm run prettier && npm run lint"
  },
  "dependencies": {
    "gatsby": "^3.13.0",
    "gatsby-plugin-sass": "^4.13.0",
    "gatsby-plugin-typescript": "^3.13.0",
    "gatsby-source-sanity": "^7.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.39.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  }
}
